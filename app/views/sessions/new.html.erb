<div class="login-container">
    <h1>Log In Here!</h1>

    <%= form_with(url: login_path, class: "login-form") do |f|%>
        <%= f.label :email %>
        <%= f.email_field :email %>
        <%= f.label :password %>
        <%= f.password_field :password %>

        <%= f.submit "Log In", class: "submit" %>
    <% end %>
</div>
